@model IEnumerable<FirstWebApiUI.Models.UserModel>
@{
    ViewData["Title"] = "User List";
}
<h2>Users</h2>

<table class="table table-striped table-bordered text-center">
    <thead class="table-dark">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.email</td>
                <td>@item.phone</td>
            </tr>
        }
    </tbody>
</table>
